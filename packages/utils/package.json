{
  "name": "@monorepo-starter/utils",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "./dist/*"
  ],
  "scripts": {
    "build": "tsc && tsc-alias",
    "format": "prettier ./src --write",
    "lint": "eslint \"src/**/*.+(ts|js|tsx|jsx)\"",
    "lint:fix": "eslint \"src/**/*.+(ts|js|tsx|jsx)\" --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run"
  },
  "exports": {
    "./*": "./dist/*.js"
  }
}

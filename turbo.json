{
  "$schema": "https://turbo.build/schema.json",
  "daemon": true,
  "tasks": {
    "dev": {
      "dependsOn": ["^build"],
      "inputs": [".env"],
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": [".env"],
      "outputs": [
        "dist",
        "*.tsbuildinfo",
        ".next",
        "build",
        ".output",
        ".vinxi"
      ],
      "cache": true
    },
    "start": {
      "dependsOn": ["build"],
      "cache": false
    },
    "format": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "lint:fix": {
      "cache": true
    },
    "type-check": {
      "cache": true
    },
    "test": {
      "cache": true
    }
  }
}
